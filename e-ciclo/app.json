{
  "expo": {
    "name": "E-Ciclo",
    "slug": "e-ciclo-mdg",
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    
    
    "splash": {
      
      "image": "./assets/splash.png",
      
      "backgroundColor": "#0fd62f",
      
      "resizeMode": "contain"
    },
    
   
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    
    
    
    "android": {
      "adaptiveIcon": {
        
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000000" 
      },
      "package": "com.tomazolv.eciclo",
      "versionCode": 2, 
      
      "splash": {
        "image": "./assets/splash.png",
        "backgroundColor": "#0fd62f",
        "resizeMode": "contain"
      }
    },
    
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1c5ad169-0b45-47a4-b830-43e600633eab"
      }
    },
    
    "plugins": [
      
      "expo-router",
      "expo-font"
    ],
    
    
    "scheme": "eciclo" 
  }
}